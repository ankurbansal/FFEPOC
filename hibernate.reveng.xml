<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<type-mapping>
		<sql-type jdbc-type="VARCHAR" hibernate-type="string"></sql-type>
		<sql-type jdbc-type="INTEGER" hibernate-type="int"
			not-null="true">
		</sql-type>
		<sql-type jdbc-type="BIT" hibernate-type="boolean"
			not-null="true">
		</sql-type>
	</type-mapping>
	<table-filter match-name="digital_costs_types"></table-filter>
	<table-filter match-name="digital_costs_vendor"></table-filter>
	<table-filter match-name="digital_costs_vendor_service"></table-filter>
	<table-filter match-name="digital_serv_temp_assoc"></table-filter>
	<table-filter match-name="digital_serv_template"></table-filter>
	<table-filter match-name="digital_service"></table-filter>
	<table-filter match-name="estimate"></table-filter>
	<table-filter match-name="estimate_cost_type"></table-filter>
	<table-filter match-name="estimate_costing"></table-filter>
	<table-filter match-name="region"></table-filter>
	<table-filter match-name="release"></table-filter>
	<table-filter match-name="territory"></table-filter>
	<table-filter match-name="title"></table-filter>
</hibernate-reverse-engineering>